{
  "name": "internify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "compile": "tsc -b",
    "compile:functions": "cd functions && npm run build",
    "deploy": "firebase deploy --only functions",
    "dev": "next dev",
    "lint": "npx eslint src functions --ext .js,.jsx,.ts,.tsx",
    "format": "npx prettier --write \"{src,functions}/**/*.{js,jsx,ts,tsx}\"",
    "format-check": "npx prettier --check \"{src,functions}/**/*.{js,jsx,ts,tsx}\"",
    "start": "next start"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/datetime": "^3.20.0",
    "@blueprintjs/docs-theme": "^3.9.0",
    "@blueprintjs/select": "^3.15.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.0.4",
    "classnames": "^2.2.6",
    "firebase": "^8.1.1",
    "next": "10.0.3",
    "postcss": "^8.1.10",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "prettier": "2.2.1",
    "typescript": "^4.1.2"
  }
}
